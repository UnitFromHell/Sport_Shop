@model Sport_Shop.Models.User
@{
    ViewBag.Title = Model.LoginUser;
}

<h2>Пользователь @Model.LoginUser</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Идентификатор</dt>
        <dd>@Model.IdUser</dd>

        <dt>Фамилия</dt>
        <dd>@Model.FirstNameUser</dd>

        <dt>Имя</dt>
        <dd>@Model.SecondNameUser</dd>

        <dt>Отчество</dt>
        <dd>@Model.MiddleNameUser</dd>

        <dt>Номер телефона</dt>
        <dd>@Model.PhoneNumber</dd>

        <dt>Адрес электронной почты</dt>
        <dd>@Model.Email</dd>
    </dl>
</div>



